        <value>
          <string>/chemlab_bvlc_fc7/cnnfc7.hdf5</string>
        </value>
      </nameValuePair>

      <nameValuePair>
        <name>DeCafListFile</name>
        <value>
          <string>/chemlab_bvlc_fc7/cnnfc7.list</string>
        </value>
