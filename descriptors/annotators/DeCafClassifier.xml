        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>

      <configurationParameter>
        <name>DeCafListFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>

      <configurationParameter>
        <name>DeCafKDTreeIndices</name>
        <type>String</type>
        <multiValued>false</multiValued>
