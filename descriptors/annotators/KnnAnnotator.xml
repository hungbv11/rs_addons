<?xml version="1.0" encoding="UTF-8"?>
<taeDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.cpp</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>rs_knnAnnotator</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>KnnAnnotator</name>
    <description/>
    <version>1.0</version>
    <vendor/>
    <configurationParameters>



<!--which should be CL (classify) or GT (groundTruth)  -->
<configurationParameter>
    <name>set_mode</name>
    <type>string</type>
    <multiValued>false</multiValued>
    <mandatory>false</mandatory>
</configurationParameter>

<!-- Have to be given when work with rsknn -->
<configurationParameter>
    <name>trainKNN_matrix</name>
    <type>string</type>
    <multiValued>false</multiValued>
    <mandatory>false</mandatory>
</configurationParameter>

<configurationParameter>
    <name>trainKNNLabel_matrix</name>
    <type>string</type>
    <multiValued>false</multiValued>
    <mandatory>false</mandatory>
</configurationParameter>

<configurationParameter>
    <name>actual_class_label</name>
    <type>string</type>
    <multiValued>false</multiValued>
    <mandatory>false</mandatory>
</configurationParameter>


    </configurationParameters>
    <configurationParameterSettings>

<!--which should be CL (classify) or GT (groundTruth)  -->
<nameValuePair>
    <name>set_mode</name>
    <value>
        <string>CL</string>
    </value>
  </nameValuePair>


<nameValuePair>
 <name>trainKNN_matrix</name>
 <value>
     <string>IAI_CNN_INS_MatTrain_obj_10_our</string>
 </value>
</nameValuePair>

<nameValuePair>
 <name>trainKNNLabel_matrix</name>
 <value>
     <string>IAI_CNN_INS_MatTrainLabel_obj_10_our</string>
 </value>
</nameValuePair>

<nameValuePair>
    <name>actual_class_label</name>
    <value>
        <string>IAI_CNN_INS_ClassLabel_obj_10_our</string>
    </value>
  </nameValuePair>


    </configurationParameterSettings>

    <typeSystemDescription>
        <imports>
            <import location="../typesystem/all_types.xml"/>
        </imports>
    </typeSystemDescription>
    <capabilities>
        <capability>
            <inputs/>
            <outputs/>
            <languagesSupported>
                <language>x-unspecified</language>
            </languagesSupported>
        </capability>
    </capabilities>
    <operationalProperties>
        <modifiesCas>true</modifiesCas>
        <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
        <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
</taeDescription>
