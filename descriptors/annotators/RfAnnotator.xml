<?xml version="1.0" encoding="UTF-8"?>
<taeDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.cpp</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>rs_rfAnnotator</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>RfAnnotator</name>
    <description/>
    <version>1.0</version>
    <vendor/>
    
    <configurationParameters>

      <configurationParameter>
        <name>set_mode</name>
        <type>string</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>

      <configurationParameter>
        <name>trained_model_name</name>
        <type>string</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>

      <configurationParameter>
        <name>actual_class_label</name>
        <type>string</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
    </configurationParameters>
  
    <configurationParameterSettings>

      <nameValuePair>
        <name>set_mode</name>
        <description>Which should be CL (To classify data, which are coming from Robosherlock pipeline)
        or GT (To set groundTruth to the data and store it in mongDB databese) </description>
        <value>        
          <string>CL</string>
        </value>
      </nameValuePair>

      <nameValuePair>
        <name>trained_model_name</name>
        <description>train_model_name can be found in path /rs_addons/trainedData/ </description>
        <value>        
          <string>IAI_CNN_rsrfModel_tango</string>
        </value>
      </nameValuePair>

      <nameValuePair>
        <name>actual_class_label</name>
        <description>actual_class_label can be found in path /rs_resources/objects_dataset/extractedFeat/,
        which is object to class map </description> 
        <value>   
          <string>IAI_CNN_ClassLabel_tango_dumb_objects</string>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <typeSystemDescription>
      <imports>
        <import location="../typesystem/all_types.xml"/>
      </imports>
    </typeSystemDescription>
    <capabilities>
      <capability>
        <inputs/>
        <outputs>
          <type allAnnotatorFeatures="true">rs.annotation.Detection</type>
          <type allAnnotatorFeatures="true">rs.annotation.Classification</type>
        </outputs>
        <inputSofas>
          <sofaName>rs.annotation.Features</sofaName>
          <sofaName>rs.scene.MergedCluster</sofaName>
        </inputSofas>
        <languagesSupported/>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
</taeDescription>
